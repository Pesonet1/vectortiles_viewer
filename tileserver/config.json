{
  "options": {
    "paths": {
      "root": "/data",
      "styles": "styles",
      "mbtiles": "mbtiles"
    },
    "frontPage": true,
    "formatQuality": {
      "jpeg": 80,
      "webp": 90,
      "pngQuantization": false,
      "png": 90
    },
    "maxScaleFactor": 3,
    "maxSize": 2048,
    "tileMargin": 0,
    "minRendererPoolSizes": [8, 4, 2],
    "maxRendererPoolSizes": [16, 8, 4],
    "pbfAlias": "pbf",
    "serveAllStyles": true,
    "serveAllFonts": true,
    "serveStaticMaps": false
  },
  "styles": {},
  "sources": {},
  "data": {
    "openmaptiles": {
      "mbtiles": "openmaptiles.mbtiles"
    }
  }
}